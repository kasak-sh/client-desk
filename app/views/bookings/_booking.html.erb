<div id="<%= dom_id booking %>">
  <p>
    <strong>Title:</strong>
    <%= booking.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= booking.description %>
  </p>

  <p>
    <strong>Scheduled at:</strong>
    <%= booking.scheduled_at %>
  </p>

  <p>
    <strong>Duration minutes:</strong>
    <%= booking.duration_minutes %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= booking.status %>
  </p>

</div>
