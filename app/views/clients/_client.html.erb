<div id="<%= dom_id client %>">
  <p>
    <strong>Name:</strong>
    <%= client.name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= client.email %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= client.phone %>
  </p>

  <p>
    <strong>Company:</strong>
    <%= client.company %>
  </p>

  <p>
    <strong>Notes:</strong>
    <%= client.notes %>
  </p>

</div>
